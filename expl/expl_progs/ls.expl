int main()
{
	decl
		int temp, fp, counter;
    str fileName;
	enddecl

	begin
    fp = exposcall("Open", "root");

    counter = 0;
    while (counter < 60) do
      temp = exposcall("Read", fp, fileName);
      if (fileName != "-1") then
        write(fileName);
      endif;
      temp = exposcall("Seek", fp, 7);
      counter = counter + 1;
    endwhile;

    temp = exposcall("Close", fp);
		return 0;
	end
}
